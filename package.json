{
  "name": "rotaverde",
  "version": "1.0.0",
  "description": "Aplicativo React Native para explorar e registrar visitas a parques nacionais",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "backend": "cd src/backend && node server.js",
    "backend:dev": "cd src/backend && nodemon server.js",
    "dev": "concurrently \"npm run backend:dev\" \"npm start\""
  },
  "dependencies": {
    "expo": "^50.0.0",
    "expo-camera": "^14.1.0",
    "expo-image-picker": "^14.7.1",
    "expo-location": "^16.5.5",
    "expo-auth-session": "^5.4.0",
    "expo-web-browser": "^12.8.2",
    "react": "^18.2.0",
    "react-native": "^0.73.0",
    "react-navigation": "^6.1.9",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "react-native-screens": "^3.27.0",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-maps": "^1.10.0",
    "axios": "^1.6.2",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "react-native-vector-icons": "^10.0.0",
    "expo-constants": "^15.4.0",
    "expo-splash-screen": "^0.26.4",
    "expo-status-bar": "^1.11.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.73.0",
    "typescript": "^5.3.3",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "mysql2": "^3.6.5",
    "multer": "^1.4.5-lts.1",
    "jsonwebtoken": "^9.1.2",
    "bcryptjs": "^2.4.3",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
